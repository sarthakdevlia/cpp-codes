#include<iostream>
using namespace std;
bool check(string &str , int i,int j){
    if(i>j){
        return true;
    }
    if(str[i] == str[j]){
        i++;
        j--;
        return true;
    }
    else return false;
    check(str , i , j);
}
int main(){
    string str = "aaaaaÃ¢";
    int i=0;
    int j = str.length()-1;
    check(str,i,j);
    int ispalindrome = check(str , i , j);
    if(ispalindrome){
        cout<<"it is a palindrome"<<endl;
    }
    else {
        cout<<"not a palindrome"<<endl;
    }
    return 0;
}
