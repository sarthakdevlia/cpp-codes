#include<iostream>
using namespace std;
int main(){
    int a[3][3] = {1 ,8,4,5, 6,7,4,3,5};
    //vector<int>ans;
    for(int col=0;col<3;col++){//col
        if(col%2==0){
            for(int row=0;row<3;row++){
                cout<<a[row][col]<<" ";
            }
        }else{
            for(int row=2;row>=0;row--){
                cout<<a[row][col]<<" ";
            }
        }
    }


    
    }
