#include<iostream>
#include<vector>
using namespace std;
int main(){
    vector<int>v;
    int a[4][5];
    int sum ;
    int max_sum=-1;
    cout<<"enter the numbers";
    for(int i=0;i<4;i++){
        for(int j=0;j<5;j++){
            cin>>a[i][j];
        }
    }
    for(int i=0;i<4;i++){
        for(int j=0;j<5;j++){
            cout<<a[i][j]<<" ";
        }cout<<endl;
    }
     for(int i=0;i<4;i++){
        sum=0;
        for(int j=0;j<5;j++){
            sum = sum + a[i][j];
           
      
     }   cout<<"ans of row"<<" ";     
         cout<<sum<<endl;  
        // v.push_back(sum);
        if(sum>max_sum){
            max_sum=sum;
        }
        
         

    }
    cout<<max_sum;
    
    return 0;

}
